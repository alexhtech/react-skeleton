{
  "name": "react-skeleton",
  "version": "1.0.0",
  "description": "A skeleton for react based applications",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node server/index.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.production.js --optimize-minimize",
    "format": "prettier --write \"src/**/*.ts\"",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "code:format": "prettier './src/**/**.{js,jsx,ts,tsx}'"
  },
  "author": "olefirenk@gmail.com",
  "license": "MIT",
  "dependencies": {
    "eslint-plugin-react": "^7.19.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router": "^5.1.1",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-webpack-plugin": "^2.5.2",
    "express": "^4.16.3",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "^5.1.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-refresh": "^0.9.0",
    "react-svg-loader": "^3.0.3",
    "terser-webpack-plugin": "^5.1.1",
    "type-fest": "^0.21.1",
    "typescript": "^4.1.5",
    "url-loader": "^4.1.0",
    "webpack": "^5.23.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.24.2",
    "webpack-merge": "^5.7.3",
    "webpack-nano": "^1.1.1"
  }
}
