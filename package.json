{
  "name": "react-skeleton",
  "version": "1.0.0",
  "description": "A skeleton for react based applications",
  "main": "index.js",
  "scripts": {
    "prestart": "yarn",
    "start": "node server/index.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.production.js --optimize-minimize",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "code:format": "prettier './src/**/**.{js,jsx,ts,tsx}'"
  },
  "author": "olefirenk@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-router": "^5.1.3",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^4.4.1",
    "history": "^4.10.1",
    "prettier-quick": "^0.0.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@hot-loader/react-dom": "^16.11.0",
    "@types/jest": "^24.0.25",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "express": "^4.16.3",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.20.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^2.0.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-svg-loader": "^3.0.3",
    "terser-webpack-plugin": "^2.3.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.2",
    "webpack-merge": "^4.1.4"
  }
}
