{
  "name": "react-skeleton",
  "version": "1.0.0",
  "description": "A skeleton for react based applications",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node server/index.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.production.js --optimize-minimize",
    "format": "prettier --write \"src/**/*.ts\"",
    "code:check": "yarn code:lint; yarn code:format --check",
    "code:clean": "yarn code:lint --fix; yarn code:format --write",
    "code:lint": "eslint --ext .js,.jsx,.ts,.tsx \"src/\"",
    "code:format": "prettier './src/**/**.{js,jsx,ts,tsx}'"
  },
  "author": "olefirenk@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-router": "^5.1.1",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^5.1.0",
    "eslint-plugin-react": "^7.19.0",
    "history": "^4.7.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.2",
    "react-is": "^16.13.1",
    "react-router": "^5.0.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^7.0.2",
    "eslint": "^6",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "express": "^4.16.3",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-svg-loader": "^3.0.3",
    "terser-webpack-plugin": "^3.0.1",
    "typescript": "^3.7.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.20.2",
    "webpack-command": "^0.5.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.2",
    "webpack-merge": "^4.1.4"
  }
}
