{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "esnext",
    "target": "es5",
    "lib": ["es6", "dom", "es2017.object"],
    "sourceMap": true,
    "allowJs": false,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": ".",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "declaration": false,
    "baseUrl": ".",
    "paths": {
      "@common": [
        "./src/components/common"
      ],
      "@common/*": [
        "./src/components/common/*"
      ],
      "@store": [
        "./src/store"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@utils":[
        "./src/utils"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@config":[
        "./config"
      ],
      "@api":[
        "./src/api"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@assets":[
        "./src/assets"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@pages":[
        "./src/pages"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@components":[
        "./src/components"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      
    }
  },
  "include": [
    "./src/**/*",
    "./config/*"
  ]
}